<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="org.openmrs.module.cohort.CohortMemberAttributeType" table="cohort_member_attribute_type">
        <id name="cohortMemberAttributeTypeId" type="java.lang.Integer" column="cohort_member_attribute_type_id">
            <generator class="native"></generator>
        </id>
        <property name="name" type="java.lang.String" length="200"></property>
        <property name="description" type="java.lang.String" length="40"/>
        <property name="format" type="java.lang.String" length="200"></property>
        <property name="uuid" type="java.lang.String"
                  length="38" unique="true"/>
        <property name="voided" type="java.lang.Boolean" column="voided" length="1" not-null="true"/>
        <property name="voidReason" type="java.lang.String"
                  column="void_reason" length="255"/>
		
		<property name="dateCreated" column="date_created" type="java.util.Date" not-null="true"/>
		<many-to-one name="creator" column ="creator" class="org.openmrs.User" not-null="true"/>
		<many-to-one name="changedBy" column="changed_by" class="org.openmrs.User" />
		<property name="dateChanged" column="date_changed" type="java.util.Date" />
		<many-to-one name="voidedBy" column="voided_by" class="org.openmrs.User" />
		<property name="dateVoided" column="date_voided" type="java.util.Date" />
    </class>
</hibernate-mapping>
